{
  "name": "frontend",
  "private": true,
  "version": "0.2.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write .",
    "type-check": "tsc --noEmit",
    "build:analyze": "npm run build && npx vite-bundle-analyzer dist/stats.html",
    "clean": "rm -rf dist",
    "validate": "node validate-deployment.js",
    "deploy:check": "npm run validate && npm run build && npm run preview",
    "setup:testdata": "tsx scripts/setupTestData.ts",
    "vercel:build": "npm run clean && npm run type-check && npm run build",
    "vercel:build:strict": "npm run clean && npm run type-check && npm run lint && npm run build",
    "vercel:preview": "npm run vercel:build && vercel --prod",
    "vercel:dev": "vercel dev",
    "optimize": "npm run build:analyze",
    "precommit": "npm run lint:fix && npm run format && npm run type-check"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.43.2",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/vite": "^4.1.11",
    "@tsparticles/engine": "^3.8.1",
    "@tsparticles/react": "^3.0.0",
    "@types/react-table": "^7.7.20",
    "clsx": "^2.1.0",
    "dayjs": "^1.11.13",
    "dexie": "^4.0.5",
    "framer-motion": "^12.19.1",
    "gsap": "^3.13.0",
    "html2pdf.js": "^0.10.1",
    "jspdf": "^3.0.1",
    "jspdf-autotable": "^5.0.2",
    "lucide-react": "^0.525.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-icons": "^5.5.0",
    "react-is": "^19.1.0",
    "react-lottie": "^1.2.10",
    "react-router-dom": "^6.22.3",
    "react-table": "^7.8.0",
    "react-tsparticles": "^2.12.2",
    "recharts": "^3.1.0",
    "shadcn-ui": "^0.9.5",
    "swiper": "^11.2.8",
    "tailwindcss": "^4.1.11",
    "tsparticles": "^3.8.1",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@types/jspdf": "^1.3.3",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@types/react-lottie": "^1.2.10",
    "@vitejs/plugin-react": "^4.6.0",
    "dotenv": "^17.2.0",
    "eslint": "^9.30.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "prettier": "^3.2.5",
    "terser": "^5.43.0",
    "tsx": "^4.20.3",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.35.1",
    "vite": "^7.0.4",
    "vite-bundle-analyzer": "^0.10.3"
  }
}
